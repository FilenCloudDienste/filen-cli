{
	"name": "filen-cli",
	"version": "0.0.1",
	"description": "Filen CLI",
	"main": "build/index.js",
	"scripts": {
		"start-dev": "tsc && node build/index.js",
		"lint": "eslint src/**/* --ext .js,.jsx,.ts,.tsx",
		"package": "tsc && esbuild build/index.js --bundle --outfile=dist/bundle.js --format=cjs --platform=node && npm run generateKey:prod && pkg .",
		"generateKey:dev": "node -e \"require('crypto').randomBytes(32, (err, buffer) => require('fs').writeFileSync('key', buffer.toString('hex')))\"",
		"generateKey:prod": "node -e \"require('crypto').randomBytes(32, (err, buffer) => require('fs').writeFileSync('dist/key', buffer.toString('hex')))\""
	},
	"bin": "dist/bundle.js",
	"pkg": {
		"outputPath": "dist",
		"targets": [
			"node18-windows-x64",
			"node18-linux-x64",
			"node18-macos-x64",
			"node18-windows-arm64",
			"node18-linux-arm64",
			"node18-macos-arm64"
		],
		"assets": [
			"dist/key"
		]
	},
	"engines": {
		"node": "18"
	},
	"keywords": [
		"filen"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FilenCloudDienste/filen-cli.git"
	},
	"author": "Filen",
	"license": "AGPLv3",
	"dependencies": {
		"@filen/sdk": "^0.1.62",
		"arg": "^5.0.2",
		"cli-progress": "^3.12.0",
		"open": "^7.4.2"
	},
	"devDependencies": {
		"@types/cli-progress": "^3.11.5",
		"@types/node": "^20.12.7",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"@yao-pkg/pkg": "^5.11.5",
		"esbuild": "^0.20.2",
		"eslint": "^8.56.0",
		"typescript": "^5.4.5"
	}
}
