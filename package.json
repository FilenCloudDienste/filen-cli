{
	"name": "filen-cli",
	"version": "0.0.1",
	"description": "Filen CLI",
	"main": "build/index.js",
	"scripts": {
		"start-dev": "tsc && node build/index.js",
		"package": "tsc && esbuild build/index.js --bundle --outfile=dist/bundle.js --format=cjs --platform=node --external:keytar && pkg -t node18 -o dist/filen.exe dist/bundle.js",
		"lint": "eslint src/**/* --ext .js,.jsx,.ts,.tsx"
	},
	"engines": {
		"node": "18"
	},
	"keywords": [
		"filen"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FilenCloudDienste/filen-cli.git"
	},
	"author": "Filen",
	"license": "AGPLv3",
	"dependencies": {
		"@filen/sdk": "^0.1.63",
		"arg": "^5.0.2",
		"cli-progress": "^3.12.0",
		"keytar": "^7.9.0",
		"open": "^7.4.2"
	},
	"devDependencies": {
		"@types/cli-progress": "^3.11.5",
		"@types/node": "^20.12.7",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"@yao-pkg/pkg": "^5.11.5",
		"esbuild": "^0.20.2",
		"eslint": "^8.56.0",
		"typescript": "^5.4.5"
	}
}
