{
  "name": "filen-cli",
  "version": "0.0.1",
  "description": "Filen CLI",
  "main": "build/index.js",
  "scripts": {
    "start-dev": "tsc && node build/index.js",
    "package": "tsc && esbuild build/index.js --bundle --outfile=dist/bundle.js --format=cjs --platform=node && pkg -t node18 -o dist/filen.exe dist/bundle.js"
  },
  "keywords": [
    "filen"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FilenCloudDienste/filen-cli.git"
  },
  "author": "Filen",
  "license": "AGPLv3",
  "dependencies": {
    "@filen/sdk": "^0.1.58",
    "@types/node": "^20.12.7",
    "@yao-pkg/pkg": "^5.11.5",
    "arg": "^5.0.2",
    "typescript": "^5.4.5"
  }
}
